{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "curly": "error",
    "no-unused-vars": [
      "error",
      { "caughtErrorsIgnorePattern": "^_", "argsIgnorePattern": "^_" }
    ]
  }
}
