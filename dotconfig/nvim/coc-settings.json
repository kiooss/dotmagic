{
  "codeLens.enable": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.rootPatterns": [
    ".git",
    ".hg",
    ".root",
    ".env",
    ".projections.json"
  ],
  "diagnostic.checkCurrentLine": false,
  "diagnostic.errorSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.hintSign": "",
  /* "diagnostic.messageTarget": "echo", */
  "eslint.autoFixOnSave": true,
  "git.enableGutters": false,
  "git.addedSign.text": "",
  "git.removedSign.text": "",
  "git.changedSign.text": "",
  "git.changeRemovedSign.text": "≃",
  "list.indicator": "❯❯❯",
  "phpls.enable": false,
  "prettier.singleQuote": true,
  "prettier.onlyUseLocalVersion": true,
  "session.directory": "~/.cache/vim/sessions",
  "solargraph.checkGemVersion": true,
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "snippets.ultisnips.enable": false,
  "suggest.echodocSupport": false,
  "suggest.snippetIndicator": "⭐︎",
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "typescript.implementationsCodeLens.enable": true,
  "typescript.referencesCodeLens.enable": false,
  "languageserver": {
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    }
  },
  "python.pythonPath": "/home/yang/.pyenv/versions/sato/bin/python",
  /* "python.venvPath": "/home/yang/.pyenv/versions/sato", */
  "Lua.diagnostics.globals": ["vim"],
  "markdownlint.onOpen": false
